run int {
    str temp = TempDir(``, `gentee_test`)
    str fname = temp + `/tmp`
    str dest =  temp + `/dest`
    WriteFile(fname, Repeat("This is a test string\n", 10000))
    int ret = CopyFile(fname, dest)
    RemoveDir(temp)
    return ret
}
