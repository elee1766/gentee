run str {
    str src = `This is тестовая строка`
    str pattern = `.is`
    src = ReplaceRegExp(ReplaceRegExp(src, pattern, `sup`), `т(.{2})`, "п${1}х")
    return ReplaceRegExp(ReplaceRegExp(src, `qwe`, `ooops`), `х(.)`, `zz`)
}
