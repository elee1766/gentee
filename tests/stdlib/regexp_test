run str {
    arr.arr.str a &= FindRegExp(`My email is xzy@somedomainname.ooops`, `(\w+)@(\w+)\.(\w+)`)
    return `%{a[0][1]}@%{a[0][2]}.%{a[0][3]} == ` + a[0][0]
}
